<!DOCTYPE query SYSTEM "__default_table_names__.txt">
<query name="account_group_parents_create" loop="//_post/account_group_parents">
    <connection>pbooks</connection>
    <params>
        <param name="account_group_get_new_id/id" type="integer"/>
        <param array="_post/account_group_parents" type="integer"/>
	</params>
    <sql>        
        INSERT INTO &pb_account_group_parents;
        (account_group_id, parent_group_id) VALUES (?,?)
    </sql>
</query>